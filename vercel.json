{
  "version": 2,
  "regions": ["hnd1"],
  "builds": [
    { "src": "wombat-handler.js","use": "@vercel/node" },
    { "src": "public/**","use": "@vercel/static" },
    { "src": "*.json","use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/","dest": "/public/index.html" },
    { "src": "/.*","dest": "/public/wombat-handler.js" }
  ]
}
